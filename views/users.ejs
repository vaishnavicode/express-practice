<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Users</title>
  </head>
  <body>
    <h1>Users</h1>

    <table title="Data" id="datatable">
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% usersList.forEach(function(user) { %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.email %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
